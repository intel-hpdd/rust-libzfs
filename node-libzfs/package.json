{
  "name": "@iml/node-libzfs",
  "version": "0.1.13",
  "description": "Neon bindings to libzfs",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intel-hpdd/node-libzfs"
  },
  "author": "IML Team",
  "license": "MIT",
  "dependencies": {
    "neon-cli": "^0.1.22"
  },
  "scripts": {
    "premock": "docker run  -dit --privileged --name mock intelhpdd/mock /usr/sbin/init",
    "mock": "cd ../ && docker cp -a ./ mock:/builddir",
    "postmock": "docker exec -ti mock bash -xec 'bash -xe cd /builddir/node-libzfs && rpkg make-source && ./mock-build.sh'",
    "install": "neon build"
  }
}